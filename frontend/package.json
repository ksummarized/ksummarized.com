{
  "name": "ksummarized",
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 8888",
    "build": "tsc && vite build",
    "serve": "vite preview --host 0.0.0.0 --port 8888",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint ./src/** ./tests/** --fix",
    "format": "prettier --write .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "license-checker-linux": "license-checker --excludePackage $(cat licenses-exclude-packages.txt) --summary --onlyAllow $(cat allowed-licenses.txt)",
    "license-checker-windows": "cd scripts && windows_check_licenses.bat"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@tailwindcss/vite": "^4.0.14",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "keycloak-js": "^23.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-router-dom": "^6.22.0",
    "tailwindcss": "^4.0.14",
    "ts-node": "^10.9.2",
    "vite": "^6.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitest/coverage-v8": "^3.0.8",
    "@vitest/eslint-plugin": "^1.1.37",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^3.9.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.0.0",
    "license-checker": "^25.0.1",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vitest": "^3.0.8"
  }
}
